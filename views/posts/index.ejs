<h1 class="title is-1">Posts</h1>


<div class="columns is-multiline">
  <% posts.forEach(post => { %>
    <div class= "column is-4">
      <a href=/posts/<%= post.id %>>
        <img src=<%= post.coverPic %> alt=<%= post.title %> />
        <h2 class= "title is-3"><%- post.title %></h2>
        <h3 class= "subtitle is-6"><%- post.subtitle %></h3>
      </a>
    </div>
    <% }) %>
</div>
